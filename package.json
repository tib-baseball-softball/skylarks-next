{
  "name": "skylarks-next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "extract": "dotenvx run -- wuchale",
    "clean": "dotenvx run -- wuchale --clean",
    "preview": "vite preview",
    "test": "dotenvx run -- vitest && pnpm test:e2e",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "generate:types": "typed-pocketbase -d src/lib/model",
    "test:e2e": "dotenvx run -- playwright test",
    "test:ui": "dotenvx run -- playwright test --ui",
    "test:unit": "dotenvx run -- vitest",
    "format": "prettier --write .",
    "lint": "prettier --check ."
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.0",
    "@playwright/test": "^1.56.1",
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/kit": "^2.48.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tigawanna/typed-pocketbase": "^0.1.6",
    "@types/leaflet": "^1.9.21",
    "@types/node": "24.9.2",
    "@vincjo/datatables": "^1.14.10",
    "@wuchale/svelte": "^0.16.6",
    "@wuchale/vite-plugin": "^0.14.8",
    "apexcharts": "^4.7.0",
    "bits-ui": "^2.14.1",
    "bsm.js": "^0.2.1",
    "flatpickr": "^4.6.13",
    "leaflet": "^1.9.4",
    "lucide-svelte": "^0.552.0",
    "pocketbase": "^0.26.3",
    "svelte": "^5.43.2",
    "svelte-check": "^4.3.3",
    "svelte-persisted-state": "^1.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^3.2.4",
    "wuchale": "^0.17.5"
  },
  "type": "module",
  "overrides": {
    "typed-pocketbase": {
      "pocketbase": "$pocketbase"
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
